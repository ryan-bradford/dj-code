<?xml version='1.0' encoding='utf-8'?>
<MixxxControllerPreset schemaVersion="1">
    <controller id="Novation Launchpad">
        <info>
            <name>Novation Launchpad</name>
            <author>bradfordr</author>
            <description>Multi page mapping for the Novation Launchpad</description>
        </info>
        <scriptfiles>
            <file filename="build/launchpad-mkii.js" functionprefix="MyController"/>
        </scriptfiles>
        <controls>
            <control>
                <key>MyController.someButton</key>
                <status>0xB0</status>
                <midino>0x68</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <key>MyController.someButton</key>
                <status>0x90</status>
                <midino>0x4A</midino>
                <options>
                    <Script-Binding/>
                </options>
            </control>
            <control>
                <group>[Master]</group>
                <key>MyController.someButton</key>
                <status>0x90</status>
                <midino>0x33</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
        </controls>
    </controller>
</MixxxControllerPreset>